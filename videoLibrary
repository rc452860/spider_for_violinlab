<style>
	.main-category {

		font-size: 20px;
		font-family: "Helvetica Neue", Arial, sans-serif;
		font-weight: 100;
	}

	.sub-category {

		padding: 5px 0 0 10px;
		display: none;
		font-size: 15px;
		font-family: "Helvetica Neue", Arial, sans-serif;
		cursor: pointer;
	}

	.sub-sub-menu {

		padding: 1px 0 10px 10px;
		font-size: 15px;
		display: none;
		font-family: "Helvetica Neue", Arial, sans-serif;
		cursor: pointer;
	}

	.sub-menu {

		padding-bottom: 3px;
	}

</style>

<script>
	$('.main-category').click(function() {

		var getId = $(this).attr('dir');

		$('.sub-category').slideUp(200);

		if ($('#sub-category-' + getId).is(":visible"))
			$('#sub-category-' + getId).slideUp();
		else
			$('#sub-category-' + getId).delay(200).slideDown(200);
		
		var request = $.ajax({

			type : 'POST',
			url : 'ajaxCategory.php',
			data : {
				category : getId
			}
		});

		request.done(function(msg) {

			$('#lesson-panel').html(msg);

		});
		
	});

	$('.sub-menu').click(function() {

		var getId = $(this).attr('dir');

		$('.sub-sub-menu').slideUp(200);

		if ($('#sub-sub-menu-' + getId).is(":visible"))
			$('#sub-sub-menu-' + getId).slideUp();
		else
			$('#sub-sub-menu-' + getId).delay(200).slideDown(200);
			
			
		var request = $.ajax({

			type : 'POST',
			url : 'ajaxBranch.php',
			data : {
				branch : getId
			}
		});

		request.done(function(msg) {

			$('#lesson-panel').html(msg);

		});
		
			
	});

	$('.select-lessons').click(function() {

		var getCategory = $(this).attr('category');
		var getTwig = $(this).attr('twig');
		//$('.lesson').hide();

		var request = $.ajax({

			type : 'POST',
			url : 'ajaxLessons.php',
			data : {
				twig : getTwig
			}
		});

		request.done(function(msg) {

			$('#lesson-panel').html(msg);

		});


	});

	$('.set-tag-words').click(function() {

		var getTag = $(this).attr('dir');
		
		var request = $.ajax({

			type : 'POST',
			url : 'ajaxLessonsTag.php',
			data : {
				tag : getTag
			}
		});

		request.done(function(msg) {
			
			$('#lesson-panel').html(msg);

		});
		
		window.scrollTo(0,0);

	});

	$('.que-lesson').live('click', function() {

		//alert('working');

		var getLesson = $(this).val();
		var getQuery = "delete";

		if ($(this).is(':checked'))
			getQuery = "insert";

		var request = $.ajax({

			type : 'POST',
			url : 'ajaxQue.php',
			data : {
				lesson : getLesson,
				query : getQuery
			}
		});

		request.done(function(msg) {
			
			//alert(msg);

			if (msg == "success") {
				// do nothing
			} else {

				alert(msg);

			}

		});

	});

</script>



		
		<!-- end of content -->

          
            
